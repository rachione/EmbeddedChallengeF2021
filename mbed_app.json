{

    "target_overrides": {
        "*": {
            "target.c_lib": "small",
            "target.printf_lib": "minimal-printf",
            "platform.minimal-printf-enable-floating-point": true,
            "platform.stdio-minimal-console-only": true,
            "target.features_add": [
                "STORAGE"
            ],
            "target.components_add": [
                "SPIF",
                "FLASH",
                "FLASHIAP"
            ],
            "target.restrict_size": "0x40000"
        }
    }
}